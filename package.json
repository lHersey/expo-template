{
  "name": "my-app",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "dev": "APP_VARIANT=development expo start",
    "preview": "APP_VARIANT=preview expo start",
    "android": "expo start --android",
    "android:dev": "APP_VARIANT=development npx expo start --android",
    "android:preview": "APP_VARIANT=preview npx expo start --android",
    "ios": "expo start --ios",
    "ios:dev": "APP_VARIANT=development npx expo start --ios",
    "ios:preview": "APP_VARIANT=preview npx expo start --ios",
    "web": "expo start --web",
    "lint": "eslint . --fix",
    "test": "jest --silent",
    "test:ci": "jest --ci --silent",
    "test:watch": "jest --watch",
    "prepare": "husky",
    "doctor": "npx expo-doctor",
    "build:dev:android": "eas build --profile development --platform android --no-wait",
    "build:dev:ios": "eas build --profile development --platform ios --no-wait",
    "build:dev:simulator": "eas build --profile simulator --platform ios --no-wait",
    "build:preview:android": "eas build --profile preview --platform android --no-wait",
    "build:preview:ios": "eas build --profile preview --platform ios --no-wait",
    "build:prod:android": "eas build --profile production --platform android --no-wait",
    "build:prod:ios": "eas build --profile production --platform ios --no-wait"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.23.1",
    "expo": "~52.0.41",
    "expo-splash-screen": "0.29.22",
    "expo-status-bar": "2.0.1",
    "expo-updates": "~0.27.4",
    "react": "18.3.1",
    "react-native": "0.76.7",
    "react-native-reanimated": "3.16.1",
    "expo-dev-client": "~5.0.14",
    "expo-system-ui": "~4.0.8"
  },
  "devDependencies": {
    "@babel/core": "7.25.2",
    "@commitlint/cli": "19.8.0",
    "@commitlint/config-conventional": "19.8.0",
    "@eslint/compat": "1.2.7",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.23.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.2.0",
    "@types/jest": "29.5.14",
    "@types/react": "~18.3.12",
    "@typescript-eslint/eslint-plugin": "8.27.0",
    "@typescript-eslint/parser": "8.27.0",
    "babel-jest": "29.7.0",
    "babel-plugin-module-resolver": "5.0.2",
    "eslint": "9.23.0",
    "eslint-config-prettier": "10.1.1",
    "eslint-plugin-jest": "28.11.0",
    "eslint-plugin-prettier": "5.2.3",
    "eslint-plugin-promise": "7.2.1",
    "eslint-plugin-react": "7.37.4",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-native": "5.0.0",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "eslint-plugin-testing-library": "7.1.1",
    "husky": "9.1.7",
    "jest": "~29.7.0",
    "jest-expo": "52.0.6",
    "lint-staged": "15.5.0",
    "prettier": "3.5.3",
    "react-test-renderer": "^18.3.1",
    "typescript": "^5.3.3"
  },
  "private": true
}
